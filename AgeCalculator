iAge = int(input('Please enter your age: '))
print(str(iAge))
if iAge >= 18 and iAge < 21:
    print('')
    print('You are an adult who is over 18')
    print('You are an adult who is still under the age of 21')
elif iAge >= 21 and iAge <= 64:
    print('')
    print("You are an adult who is over 21")
elif iAge >= 65:
    print('')
    print('You are a senior who is of the age of 65 or older')
else:
    print('')
    print("You are a minor who is under 18")
